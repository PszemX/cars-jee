# Get all brands.

GET http://localhost:9082/cars/api/brands

###

# Get single brand.

GET http://localhost:9082/cars/api/brands/cd42d39a-e527-4f30-86f7-4f8b75ab285c

###

# delete brand.

DELETE http://localhost:9082/cars/api/brands/cd42d39a-e527-4f30-86f7-4f8b75ab285c

###

# create or update brand

PUT http://localhost:9082/cars/api/brands/cd42d39a-e527-4f30-86f7-4f8b75ab285c
Content-Type: application/json

{
  "name": "BMW XL123",
  "manual": true,
  "body": "Suv"
}

###

# Get all cars.

GET http://localhost:9082/cars/api/cars


###

# Get all cars of a given brand

GET http://localhost:9082/cars/api/brands/cd42d39a-e527-4f30-86f7-4f8b75ab285c/cars

###

